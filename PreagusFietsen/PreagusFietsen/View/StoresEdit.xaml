<Window x:Class="PreagusFietsen.View.StoresEdit"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PreagusFietsen.View"
        mc:Ignorable="d"
        Title="Edit Store List" Height="450" Width="800" ResizeMode="NoResize">
    <Grid Margin="0,0,10,10">
        <DataGrid x:Name="StoresDataGrid" ItemsSource="{Binding Stores}" HorizontalAlignment="right" Height="399" Margin="0,10,119,0" VerticalAlignment="Top" Width="653"/>

    </Grid>
</Window>
